[pylint]
disable=
    broad-except,
    consider-iterating-dictionary,
    consider-using-dict-items,
    consider-using-f-string,
    import-error,
    line-too-long,
    too-many-arguments,
    too-many-branches,
    too-many-lines,
    too-many-locals,
    too-many-return-statements,
    too-many-statements,
    unnecessary-dict-index-lookup,
    unspecified-encoding,
good-names=
    github-util
ignore=
    __init__.py,
